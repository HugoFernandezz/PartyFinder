{
  "expo": {
    "name": "Jaleo",
    "slug": "jaleo-murcia",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/IconPartyFinder.png",
    "userInterfaceStyle": "automatic",
    "extra": {
      "eas": {
        "projectId": "0a49467e-1eae-4cb0-8aa7-8902253891c3"
      }
    },
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jaleo.app",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.jaleo.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/IconPartyFinder.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "RECEIVE_BOOT_COMPLETED"
      ]
    },
    "web": {
      "favicon": "./assets/IconPartyFinder.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/IconPartyFinder.png",
          "color": "#ffffff",
          "sounds": []
        }
      ]
    ],
    "owner": "hugofernandezz3"
  }
}
